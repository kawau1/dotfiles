{
    "context": {
        "fileName": "AGENTS.md",
        "loadMemoryFromIncludeDirectories": true
    },
    "mcpServers": {},
    "tools": {
        "autoAccept": true,
        "exclude": [
            "ReadFileTool(./.env)",
            "ReadFileTool(./.env.*)",
            "ReadFileTool(./secrets/**)",
            "ReadFileTool(./config/credentials.json)",
            "ReadFileTool(./build)",
            "ReadFileTool(.ssh)",
            "ReadFileTool(id_rsa)",
            "ReadFileTool(id_ed25519)",
            "ReadFileTool(**/*token*)",
            "ReadFileTool(**/*key*)",
            "ShellTool(sudo *)",
            "ShellTool(rm *)",
            "ShellTool(rm -rf *)",
            "ShellTool(git push *)",
            "ShellTool(git commit *)",
            "ShellTool(git reset *)",
            "ShellTool(git rebase *)",
            "WriteFileTool(.env*)",
            "WriteFileTool(**/secrets/**)"
        ],
        "shell": {
            "showColor": true
        },
        "enableMessageBusIntegration": true
    },
    "general": {
        "checkpointing": {
            "enabled": true
        },
        "previewFeatures": true,
        "enablePromptCompletion": true
    },
    "ui": {
        "hideFooter": false,
        "showMemoryUsage": true,
        "showLineNumbers": true,
        "theme": "Default",
        "showStatusInTitle": true,
        "footer": {
            "hideContextPercentage": false
        },
        "showCitations": true,
        "showModelInfoInChat": true,
        "useAlternateBuffer": true
    },
    "ide": {
        "enabled": true
    },
    "security": {
        "auth": {
            "selectedType": "oauth-personal"
        }
    },
    "output": {
        "format": "text"
    }
}
